{
    "name": "ssilence/selfoss",
    "description": "Multipurpose rss reader, live stream, mashup, aggregation web application",
    "type": "project",
    "require": {
        "php": ">= 5.6",
        "ext-gd": "*",
        "bcosca/fatfree-core": "^3.7.0",
        "bramus/router": "^1.6",
        "danielstjules/stringy": "^3.1",
        "fossar/guzzle-transcoder": "^0.2.0",
        "fossar/htmlawed": "^1.2.4.1",
        "guzzlehttp/guzzle": "^7.4",
        "guzzlehttp/oauth-subscriber": "^0.6.0",
        "guzzlehttp/psr7": "^2.1",
        "http-interop/response-sender": "^1.0",
        "j0k3r/graby": "^2.0.0",
        "level-2/dice": "^4.0",
        "lordelph/icofileloader": "^2.0",
        "mibe/feedwriter": "^1.0",
        "monolog/monolog": "^2.3",
        "php-http/guzzle7-adapter": "^1.0",
        "simplepie/simplepie": "^1.3",
        "smottt/wideimage": "^1.1",
        "violet/streaming-json-encoder": "^1.1",
        "willwashburn/phpamo": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5",
        "php-parallel-lint/php-parallel-lint": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "SSilence",
            "email": "tobias.zeising@aditu.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "controllers\\": "src/controllers/",
            "daos\\": "src/daos/",
            "helpers\\": "src/helpers/",
            "spouts\\": "src/spouts/",
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "cs": "php-cs-fixer fix --verbose --dry-run --diff",
        "fix": "php-cs-fixer fix --verbose --diff",
        "lint": "parallel-lint src tests",
        "test": "phpunit --bootstrap tests/bootstrap.php tests"
    }
}
